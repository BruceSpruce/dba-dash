  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DBAChecksDB</DataSourceName>
        <CommandText>SELECT I.Instance, SCO.configuration_id,
                  SCO.name,
                  SCO.default_value,
				  SC.value,
				  CASE WHEN SC.value=SCO.default_value THEN 1 ELSE 0 END AS IsDefault
FROM dbo.SysConfig SC
JOIN dbo.Instances I ON SC.InstanceID=I.InstanceID
JOIN dbo.SysConfigOptions SCO ON SC.configuration_id = SCO.configuration_id
WHERE I.IsActive=1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Instance">
          <DataField>Instance</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="configuration_id">
          <DataField>configuration_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="default_value">
          <DataField>default_value</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="IsDefault">
          <DataField>IsDefault</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
