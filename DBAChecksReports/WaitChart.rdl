  <DataSources>
    <DataSource Name="DBAChecksDB">
      <DataSourceReference>DBAChecksDB</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>99b020d7-e78d-46d3-8e06-5ccc3f3e37b9</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DBAChecksDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Mins">
            <Value>=Parameters!Mins.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InstanceID">
            <Value>=Parameters!InstanceID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH T AS (
	SELECT WT.WaitType,SUM(W.wait_time_ms) TotalWaitMs,SUM(W.wait_time_ms)*1.0/ SUM(SUM(W.wait_time_ms)) OVER() Pct
	FROM dbo.Waits W 
	JOIN dbo.WaitType WT ON WT.WaitTypeID = W.WaitTypeID
	WHERE SnapshotDate&gt;= DATEADD(mi,-@Mins,GETUTCDATE())
	AND WT.WaitType &lt;&gt;'REDO_THREAD_PENDING_WORK'
AND InstanceID = @InstanceID
	GROUP BY WT.WaitType
)
SELECT * FROM T 
WHERE pct&gt;0.01
ORDER BY T.TotalWaitMs desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="WaitType">
          <DataField>WaitType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalWaitMs">
          <DataField>TotalWaitMs</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Pct">
          <DataField>Pct</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
